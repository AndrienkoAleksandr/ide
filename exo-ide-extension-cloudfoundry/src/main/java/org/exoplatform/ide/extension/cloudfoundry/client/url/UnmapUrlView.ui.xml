<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component"
             xmlns:e="urn:import:org.exoplatform.ide.extension.cloudfoundry.client.url">

   <ui:with field='resource' type='org.exoplatform.ide.extension.cloudfoundry.client.CloudFoundryClientBundle' />
   <ui:with field="locale" type="org.exoplatform.ide.extension.cloudfoundry.client.CloudFoundryLocalizationConstant"></ui:with>

   <f:VPanel>
      <f:Border margin="10">
         <g:ScrollPanel>
         <e:RegisteredUrlsGrid width="100%" ui:field="registeredUrlsGrid"></e:RegisteredUrlsGrid>
         </g:ScrollPanel>
      </f:Border>

      <g:HorizontalPanel height="30px" horizontalAlignment="center">
         <f:ImageButton ui:field="deleteButton" width="100" height="22" text="{locale.deleteButton}" imageResource="{resource.deleteButton}"
            disabledImageResource="{resource.deleteButtonDisabled}"></f:ImageButton>
            <f:ImageButton ui:field="closeButton" width="100" height="22" text="{locale.closeButton}" imageResource="{resource.cancelButton}"
            disabledImageResource="{resource.cancelButtonDisabled}"></f:ImageButton>
      </g:HorizontalPanel>
   </f:VPanel>
</ui:UiBinder> 