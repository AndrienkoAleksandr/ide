                  <exclude>**/codemirror/js/*.js</exclude>
                  <exclude>**/codemirror2/**/*.js</exclude>
                  
		  <exclude>**/ckeditor/**/*.css</exclude>
                  <exclude>**/ckeditor/**/*.html</exclude>
                  <exclude>**/ckeditor/**/*.js</exclude>
		  
		  <exclude>exo-ide-editor-ruby/src/main/resources/org/exoplatform/ide/editor/ruby/client/hello.rb</exclude>
                  
		  <exclude>exo-ide-client/src/main/resources/org/exoplatform/ide/client/samples/**</exclude>
                                    
                  <exclude>exo-ide-packaging-shell-war/src/main/resources/WEB-INF/crash/ssh/hostkey.pem</exclude>
                  
		  <exclude>exo-ide-extension-git/src/main/java/org/exoplatform/ide/git/server/jgit/jgit_copy/CheckoutResult_Copy.java</exclude>
                  <exclude>exo-ide-extension-git/src/main/java/org/exoplatform/ide/git/server/jgit/jgit_copy/CheckoutCommand_Copy.java</exclude>
                  <exclude>exo-ide-extension-git/src/main/java/org/exoplatform/ide/git/server/jgit/jgit_copy/DirCacheCheckout_Copy.java</exclude>
                  
                  <exclude>exo-ide-extension-java-runtime/src/main/java/Java.tokens</exclude>
                  <exclude>exo-ide-extension-java-runtime/src/main/java/JavaTreeParser.tokens</exclude>
                  <exclude>exo-ide-extension-java-runtime/src/main/resources/org/exoplatform/ide/extension/java/jdi/server/expression/JavaTreeParser.g</exclude>
                  <exclude>exo-ide-extension-java-runtime/src/main/resources/org/exoplatform/ide/extension/java/jdi/server/expression/Java.g</exclude>
                  <exclude>exo-ide-extension-java/src/main/resources/pom.xslt</exclude>
                  <exclude>exo-ide-extension-jenkins/src/main/resources/jenkins-config.xslt</exclude>
                  
                  <exclude>exo-ide-shell/src/main/java/org/exoplatform/ide/shell/client/cli/**</exclude>
		  
		  <exclude>**/catalina.policy</exclude>
                  <exclude>**/server.xml</exclude>
                  <exclude>**/jaas.conf</exclude>
                  
		  <exclude>**/*.txt</exclude>
		  <exclude>**/*.json</exclude>
		  <exclude>**/.gwt/**</exclude>
		  <exclude>**/LICENSE</exclude>
                  <exclude>.idea/**</exclude>
                  <exclude>README</exclude>
		  <exclude>.gitignore</exclude>
               </excludes>
               <mapping>
                  <java>SLASHSTAR_STYLE</java>
               </mapping>
            </configuration>
            <executions>
               <execution>
                  <id>check-headers</id>
                  <phase>verify</phase>
                  <goals>
                     <goal>check</goal>
                  </goals>
               </execution>
            </executions>
         </plugin>
      </plugins>
   </build>
