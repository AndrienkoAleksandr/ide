      <plugins>
         <plugin>
            <inherited>false</inherited>
            <groupId>com.mycila.maven-license-plugin</groupId>
            <artifactId>maven-license-plugin</artifactId>
            <version>1.9.0</version>
            <configuration>
               <header>exo-ide-dev-resources/src/main/resources/licences/header.txt</header>
               <validHeaders>
                  <validHeader>exo-ide-dev-resources/src/main/resources/licences/header2010.txt</validHeader>
                  <validHeader>exo-ide-dev-resources/src/main/resources/licences/header2010_now.txt</validHeader>
                  <validHeader>exo-ide-dev-resources/src/main/resources/licences/header2011.txt</validHeader>
                  <validHeader>exo-ide-dev-resources/src/main/resources/licences/header2012.txt</validHeader>
                  <validHeader>exo-ide-dev-resources/src/main/resources/licences/headerApache.txt</validHeader>
               </validHeaders>
               <failIfMissing>false</failIfMissing>
               <aggregate>true</aggregate>
               <strictCheck>true</strictCheck>
               <useDefaultExcludes>true</useDefaultExcludes>
               <properties>
                  <year>2012</year>
               </properties>
               <excludes>
		  <exclude>exo-ide-docs/**</exclude>
                  <exclude>exo-ide-jdt-core/**</exclude>
		  <exclude>exo-ide-dev-resources/**</exclude>
		  
		  <exclude>**/src/test/resources/**</exclude>
                  
		  <exclude>**/codemirror/css/*.css</exclude>
