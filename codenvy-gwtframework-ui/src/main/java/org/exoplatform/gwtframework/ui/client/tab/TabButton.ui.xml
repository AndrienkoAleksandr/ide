<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:with type='org.exoplatform.gwtframework.ui.client.tab.TabButton.Style' field="style"/>
    <g:HTML styleName="{style.controlPanel}" ui:field="controlPanel">

        <table class="{style.controlTable}" border="0" cellpadding="0" cellspacing="0">
            <tr>
                <td class="{style.controlCell}">
                    <div class="{style.button}" ui:field="buttonPanel">
                        <div class="{style.iconPanel}" ui:field="iconPanel"></div>
                    </div>
                </td>
            </tr>
        </table>

    </g:HTML>

</ui:UiBinder>
