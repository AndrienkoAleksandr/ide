<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:HTMLPanel styleName="tabPanelScroller">
        <table class="tabPanelScrollerTable" cellpadding="0" cellspacing="0" border="0">
            <tr>
                <td>
                    <div style="width: 1px; height: 1px;"></div>
                </td>

                <td>
                    <g:HTML ui:field="scrollLeft" styleName="tabPanelScrollerLeft"></g:HTML>
                </td>

                <td>
                    <g:HTML ui:field="scrollRight" styleName="tabPanelScrollerRight"></g:HTML>
                </td>
            </tr>
        </table>
    </g:HTMLPanel>

</ui:UiBinder> 
