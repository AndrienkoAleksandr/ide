<ui:UiBinder
        xmlns:ui='urn:ui:com.google.gwt.uibinder'
        xmlns:g='urn:import:com.google.gwt.user.client.ui'
        >

    <g:HTMLPanel styleName='testCasePanel'>
        <table cellpadding="5" border="0" style="width: 100%; height: 100%;">
            <tbody>
                <tr>

                    <td>
                        <g:FlowPanel ui:field="testCasePanel" styleName="testCaseUIPanel"></g:FlowPanel>
                    </td>

                    <td style="padding: 5px; width: 250px; text-align: center;" ui:field="controlsElement">
                        <g:FlowPanel ui:field="controlsPanel" styleName="testCaseControlsPanel"></g:FlowPanel>
                    </td>

                </tr>
            </tbody>
        </table>
    </g:HTMLPanel>

</ui:UiBinder>
