<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.component {
			float: left;
		}
	</ui:style>
	<g:FlowPanel width="100%" height="100%" ui:field="mainPanel"
		styleName="{style.component}">
		<g:Label text="Project name:" horizontalAlignment="ALIGN_LEFT"
			width="" styleName="{style.component}" />
		<g:TextBox ui:field="projectName" visibleLength="30"
			styleName="component" />
	</g:FlowPanel>
</ui:UiBinder> 