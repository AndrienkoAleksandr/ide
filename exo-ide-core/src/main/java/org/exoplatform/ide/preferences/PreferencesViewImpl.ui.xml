<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.btnAlign {
			float: right;
		}
		
		.position {
			position: static;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="600px" height="400px">
		<g:south size="26.0">
			<g:FlowPanel height="100%" width="100%">
				<g:Button styleName="{style.btnAlign}" text="Close" ui:field="btnClose"/>
				<g:Button styleName="{style.btnAlign}" text="Ok" ui:field="btnOk"/>
				<g:Button styleName="{style.btnAlign}" text="Apply" ui:field="btnApply"/>
			</g:FlowPanel>
		</g:south>
		<g:west size="180.0">
			<g:ScrollPanel width="100%" height="100%" ui:field="preferences"/>
		</g:west>
		<g:center>
			<g:SimplePanel width="100%" height="100%" ui:field="contentPanel"/>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 