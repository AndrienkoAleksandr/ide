<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module>
   <!-- Generator that builds ExtensionManager from ExtensionDefinition annotations. -->
   <generate-with class="org.exoplatform.ide.extension.ExtensionRegistryGenerator">
      <when-type-assignable class="org.exoplatform.ide.extension.ExtensionRegistry" />
   </generate-with>

   <inherits name="com.google.gwt.user.User" />
   
   <inherits name="elemental.Elemental" />
   <inherits name="org.exoplatform.ide.Commons" />
   <inherits name="com.google.gwt.json.JSON" />
   <inherits name="com.google.gwt.inject.Inject" />

   <source path="ui" />
   <source path="" />
</module>