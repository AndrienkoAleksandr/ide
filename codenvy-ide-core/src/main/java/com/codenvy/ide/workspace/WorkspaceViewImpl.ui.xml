<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >
    <ui:style>
            /* Add CSS here. See the GWT docs on UI Binder for more details */
        .important {
            font-weight: bold;
        }

        .position {
            position: static;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="100%" height="100%">
        <g:north size="25">
            <g:SimplePanel width="100%" ui:field="menuPanel"/>
        </g:north>
        <g:north size="30">
            <g:SimplePanel width="100%" ui:field="toolbarPanel"/>
        </g:north>
        <g:south size="22">
            <g:FlowPanel width="100%" ui:field="statusPanel"/>
        </g:south>
        <g:center>
            <g:SimplePanel ui:field="perspectivePanel" width="100%" height="100%">
                <!-- p1:CellTree width="100%" height="100%" ui:field="cellTree" / -->
            </g:SimplePanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder> 