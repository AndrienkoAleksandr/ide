<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:DockLayoutPanel unit="PX" width="100%" height="100%">
        <g:north size="25">
            <g:DockLayoutPanel>
                <g:east size="60">
                    <g:FlowPanel>
                        <g:Button ui:field="btnLogin" text="Login" width="60px" height="25px"/>
                        <g:Button ui:field="btnLogout" text="Logout" width="60px" height="25px"/>
                    </g:FlowPanel>
                </g:east>
                <g:center>
                    <g:SimplePanel width="100%" ui:field="menuPanel"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="30">
            <g:SimplePanel width="100%" ui:field="toolbarPanel"/>
        </g:north>
        <g:south size="22">
            <g:FlowPanel width="100%" ui:field="statusPanel"/>
        </g:south>
        <g:center>
            <g:SimplePanel ui:field="perspectivePanel" width="100%" height="100%"/>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder> 