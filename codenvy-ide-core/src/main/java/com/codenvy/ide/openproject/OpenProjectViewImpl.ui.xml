<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.codenvy.ide.ui">
    <ui:style>
        .alignBtn {
            float: right;
            margin-left: 5px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="300px" height="300px">
        <g:south size="26">
            <g:FlowPanel>
                <c:Button addStyleNames="{style.alignBtn}" size="SMALL" type="SUCCESS" text="Open" ui:field="btnOpen"/>
                <c:Button addStyleNames="{style.alignBtn}" size="SMALL" type="DANGER" text="Cancel" ui:field="btnCancel"/>
            </g:FlowPanel>
        </g:south>
        <g:center>
            <g:ScrollPanel ui:field="listPanel"/>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder> 