<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
   xmlns:c="urn:import:org.exoplatform.gwtframework.ui.client.component" xmlns:tp="urn:import:org.exoplatform.gwtframework.ui.client.tab">

   <g:LayoutPanel>
      <g:layer top="0" bottom="50px">
         <g:HorizontalPanel width="100%" height="100%">
            <c:Border margin="5" borderColor="transparent">
               <c:layer>
                  <tp:TabPanel ui:field="jarsTabPanel"></tp:TabPanel>
               </c:layer>
            </c:Border>

            <c:Border margin="5" borderColor="transparent">
               <c:layer>
                  <tp:TabPanel ui:field="propertiesTabPanel"></tp:TabPanel>
               </c:layer>
            </c:Border>

         </g:HorizontalPanel>
      </g:layer>
      <g:layer bottom="0" height="50px">
         <g:HorizontalPanel spacing="5" width="100%" height="50px">
            <g:cell horizontalAlignment='ALIGN_CENTER' verticalAlignment="ALIGN_MIDDLE">
               <c:ImageButton text="OK" imageName="ok" ui:field="okButton" buttonId="exoJarLibrariesOkButton"></c:ImageButton>
            </g:cell>
         </g:HorizontalPanel>
      </g:layer>
   </g:LayoutPanel>

</ui:UiBinder>
