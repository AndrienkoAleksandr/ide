<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:org.exoplatform.gwtframework.ui.client.component"
	xmlns:tp="urn:import:org.exoplatform.gwtframework.ui.client.tab"
	>

	<c:VPanel>
	
		<c:HPanel>
			 <c:Border margin="5" borderColor="transparent">
			 	<tp:TabPanel ui:field="jarsTabPanel"></tp:TabPanel>
			 </c:Border>
			
			<c:Border margin="5" borderColor="transparent">
				<tp:TabPanel ui:field="propertiesTabPanel"></tp:TabPanel>
			</c:Border>
		</c:HPanel>
		
		<g:HorizontalPanel spacing="5" width="100%" height="50px">
			<g:cell horizontalAlignment='ALIGN_CENTER' verticalAlignment="ALIGN_MIDDLE">
				<c:ImageButton text="OK" imageName="ok" ui:field="okButton" buttonId="exoJarLibrariesOkButton"></c:ImageButton>				
			</g:cell>
		</g:HorizontalPanel>			
		
	</c:VPanel>
	
</ui:UiBinder>
