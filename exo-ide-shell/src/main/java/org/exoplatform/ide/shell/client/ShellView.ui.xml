             xmlns:s="urn:import:org.exoplatform.ide.shell.client">
    <ui:with field='resource' type='org.exoplatform.ide.shell.client.ShellClientBundle'/>
    <ui:style>

    </ui:style>

    <g:FlowPanel styleName="{resource.css.shellContainer}">
