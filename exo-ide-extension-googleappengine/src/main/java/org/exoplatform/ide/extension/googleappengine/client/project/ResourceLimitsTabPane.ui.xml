<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component"
             xmlns:e="urn:import:org.exoplatform.ide.extension.googleappengine.client.project">
    <ui:with field='locale' type='org.exoplatform.ide.extension.googleappengine.client.GAELocalization'/>

    <f:Border borderColor="transparent">
        <f:layer top="0" bottom="0">
            <f:Border margin="10">
                <f:layer>
                    <e:ResourceLimitsGrid width="100%" height="100%" ui:field="resourceLimitsGrid"></e:ResourceLimitsGrid>
                </f:layer>
            </f:Border>
        </f:layer>
    </f:Border>
</ui:UiBinder>
