<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component">
   <ui:with field='locale' type='org.exoplatform.ide.extension.googleappengine.client.GAELocalization' />
   <ui:style>
      
   </ui:style>
   <f:Border margin="15" borderColor="transparent">
      <f:layer>
         <g:DockLayoutPanel width="100%" unit="PX" height="22px">
            <g:west size="230">
               <g:CheckBox ui:field="useExistedField" HTML="{locale.deployApplicationUseExistedIdField}"></g:CheckBox>
            </g:west>
            <g:center>
               <f:TextInput ui:field="applicationIdField" width="100%" height="22px" enabled="false"></f:TextInput>
            </g:center>
         </g:DockLayoutPanel>
      </f:layer>
   </f:Border>
</ui:UiBinder> 