<?xml version="1.0" encoding="UTF-8"?>
<templates>
   <!-- project templates -->
   <template>
      <name>linkedin-contacts-project</name>
      <type>project</type>
      <src>org/exoplatform/ide/template/samples/linkedin</src>
   </template>
   <template>
      <name>shopping-cart-project</name>
      <type>project</type>
      <src>org/exoplatform/ide/template/samples/sc</src>
   </template>
   <template>
      <name>twitter-trends-project</name>
      <type>project</type>
      <src>org/exoplatform/ide/template/samples/twittertrends</src>
   </template>
   <!-- base file templates -->
   <template>
      <name>chromattic-do.groovy</name>
      <type>file</type>
      <src>base/chromattic-do.groovy</src>
      <mime-type>application/x-chromattic+groovy</mime-type>
   </template>
   <template>
      <name>gadget.xml</name>
      <type>file</type>
      <src>base/gadget.xml</src>
      <mime-type>application/x-google-gadget</mime-type>
      <jcr-content-node-type>exo:googleGadget</jcr-content-node-type>
   </template>
   <template>
      <name>GTMPL.gtmpl</name>
      <type>file</type>
      <src>base/GTMPL.gtmpl</src>
      <mime-type>application/x-groovy+html</mime-type>
   </template>
   <template>
      <name>html.html</name>
      <type>file</type>
      <src>base/html.html</src>
      <mime-type>text/html</mime-type>
   </template>
   <template>
      <name>netvibes.html</name>
      <type>file</type>
      <src>base/netvibes.html</src>
      <mime-type>application/x-uwa-widget</mime-type>
   </template>
   <template>
      <name>rest-service.grs</name>
      <type>file</type>
      <src>base/rest-service.grs</src>
      <mime-type>application/x-jaxrs+groovy</mime-type>
      <jcr-content-node-type>exo:groovyResourceContainer</jcr-content-node-type>
   </template>
   <!-- netvibes file templates -->
   <template>
      <name>SampleBlogPostWidget.html</name>
      <type>file</type>
      <src>netvibes/SampleBlogPostWidget.html</src>
      <mime-type>application/x-uwa-widget</mime-type>
   </template>
   <template>
      <name>SampleChartWidget.html</name>
      <type>file</type>
      <src>netvibes/SampleChartWidget.html</src>
      <mime-type>application/x-uwa-widget</mime-type>
   </template>
   <template>
      <name>SampleFlashWidget.html</name>
      <type>file</type>
      <src>netvibes/SampleFlashWidget.html</src>
      <mime-type>application/x-uwa-widget</mime-type>
   </template>
   <template>
      <name>SampleTabbedWidget.html</name>
      <type>file</type>
      <src>netvibes/SampleTabbedWidget.html</src>
      <mime-type>application/x-uwa-widget</mime-type>
   </template>
</templates>