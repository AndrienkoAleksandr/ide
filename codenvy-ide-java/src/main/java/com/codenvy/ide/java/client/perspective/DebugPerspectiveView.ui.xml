<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >
    <g:SplitLayoutPanel ui:field="splitPanel" width="100%" height="100%">
        <g:north size="240">
            <g:SplitLayoutPanel width="100%" height="100%">
                <g:center>
                    <g:SimplePanel ui:field="naviPanel" width="100%" height="100%"/>
                </g:center>
                <g:east size="240">
                    <g:SimplePanel ui:field="infoPanel" width="100%" height="100%"/>
                </g:east>
            </g:SplitLayoutPanel>
        </g:north>

        <g:center>
            <g:SimplePanel ui:field="editorPanel"/>
        </g:center>
        <g:east size="240">
            <g:SimplePanel ui:field="toolPanel" width="100%" height="100%"/>
        </g:east>

    </g:SplitLayoutPanel>
</ui:UiBinder> 