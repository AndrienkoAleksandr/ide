<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:style>
        .textBox {
            padding: 0;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX">
        <g:north size="16">
            <g:Label>Parent:</g:Label>
        </g:north>
        <g:north size="35">
            <g:ListBox visibleItemCount="1" ui:field="parents" width="100%" height="24px"/>
        </g:north>
        <g:north size="16">
            <g:Label>Name:</g:Label>
        </g:north>
        <g:north size="26">
            <g:TextBox ui:field="typeName" width="555px" height="20px" styleName="{style.textBox}"/>
        </g:north>
        <g:north size="16">
            <g:Label>Type:</g:Label>
        </g:north>
        <g:north size="24">
            <g:ListBox visibleItemCount="1" ui:field="types" width="100%" height="24px"/>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>