<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:org.exoplatform.gwtframework.ui.client.component"
   xmlns:ssh="urn:import:org.exoplatform.ide.extension.ssh.client.keymanager.ui">
   <ui:with type="org.exoplatform.ide.extension.ssh.client.SshLocalizationConstant" field="con"></ui:with>

   <c:VPanel>

      <c:Border margin="5">
         <ssh:SshKeysGrid ID="ideSshKeysGrid" ui:field="keysGrid"></ssh:SshKeysGrid>
      </c:Border>

      <c:Panel height="40px">
         <g:HorizontalPanel spacing="5" width="100%" height="100%">
            <g:cell horizontalAlignment='ALIGN_RIGHT' verticalAlignment="ALIGN_MIDDLE">
               <c:ImageButton text='{con.managerUploadButton}' ui:field="uploadButton" buttonId="ideSshKeyManagerUploadButton"></c:ImageButton>
            </g:cell>
            <g:cell horizontalAlignment='ALIGN_RIGHT' verticalAlignment="ALIGN_MIDDLE">
               <c:ImageButton text='{con.managerGenerateButton}' ui:field="generateButton" buttonId="ideSshKeyManagerCloseButton"></c:ImageButton>
            </g:cell>
            <g:cell horizontalAlignment='ALIGN_RIGHT' verticalAlignment="ALIGN_MIDDLE">
               <c:ImageButton text='{con.closeButton}' ui:field="closeButton" buttonId="ideSshKeyManagerCloseButton"></c:ImageButton>
            </g:cell>
         </g:HorizontalPanel>
      </c:Panel>
   </c:VPanel>

</ui:UiBinder> 