<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
   xmlns:c="urn:import:org.exoplatform.gwtframework.ui.client.component">
   <ui:with type="org.exoplatform.ide.extension.ssh.client.SshLocalizationConstant" field="con"></ui:with>
   <g:LayoutPanel>
      <g:layer top="0" bottom="40px">
         <c:Border>
            <c:layer>
               <c:TextAreaItem ui:field="publicSshKeyField" showTitle="false" name="exoSshPublicKeyField"></c:TextAreaItem>
            </c:layer>
         </c:Border>
      </g:layer>
      <g:layer height="40px" bottom="0">
         <g:HorizontalPanel spacing="5" width="100%" height="100%">
            <g:cell horizontalAlignment='ALIGN_RIGHT' verticalAlignment="ALIGN_MIDDLE">
               <c:ImageButton text='{con.closeButton}' ui:field="closeButton" buttonId="ideSshKeyManagerCloseButton"></c:ImageButton>
            </g:cell>
         </g:HorizontalPanel>
      </g:layer>
   </g:LayoutPanel>
</ui:UiBinder>