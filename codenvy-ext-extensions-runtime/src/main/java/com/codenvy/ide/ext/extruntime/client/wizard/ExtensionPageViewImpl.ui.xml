<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field='res' type='com.codenvy.ide.ext.extruntime.client.ExtRuntimeResources'/>
    <ui:style>
        .spacing {
            margin-bottom: 6px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="100%" height="100%">
        <g:north size="30.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{res.extensionRuntimeCss.login} {style.spacing}">
                <g:west size="80.0">
                    <g:Label text="GroupId:" height="18px" addStyleNames="{res.extensionRuntimeCss.loginFont}"/>
                </g:west>
                <g:center>
                    <g:TextBox ui:field="groupIdField" width="97%" height="16px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="30.0">
            <g:DockLayoutPanel unit="PX" width="100%" addStyleNames="{res.extensionRuntimeCss.login} {style.spacing}">
                <g:west size="80.0">
                    <g:Label text="ArtifactId:" height="18px" addStyleNames="{res.extensionRuntimeCss.loginFont}"/>
                </g:west>
                <g:center>
                    <g:TextBox ui:field="artifactIdField" width="97%" height="16px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="30.0">
            <g:DockLayoutPanel unit="PX" width="100%" addStyleNames="{res.extensionRuntimeCss.login} {style.spacing}">
                <g:west size="80.0">
                    <g:Label text="Version:" height="18px" addStyleNames="{res.extensionRuntimeCss.loginFont}"/>
                </g:west>
                <g:center>
                    <g:TextBox ui:field="versionField" width="97%" height="16px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>