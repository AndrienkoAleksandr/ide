<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <g:DockLayoutPanel unit="PX" width="100%" height="100%">
        <g:north size="30.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="150.0">
                    <g:Label text="Target:" height="18px"/>
                </g:west>
                <g:center>
                    <g:ListBox ui:field="targetField" width="100%" height="22px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="30.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="150.0">
                    <g:Label text="Application name:" height="18px"/>
                </g:west>
                <g:center>
                    <g:TextBox ui:field="nameField" width="100%" height="22px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
        <g:north size="30.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="150.0">
                    <g:Label text="Location:" height="18px"/>
                </g:west>
                <g:center>
                    <g:TextBox ui:field="urlField" width="100%" height="22px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>