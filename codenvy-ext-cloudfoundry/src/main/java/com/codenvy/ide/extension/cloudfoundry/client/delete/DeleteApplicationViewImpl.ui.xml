<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.floatLeft {
			float: left;
		}
		
		.floatRight {
			float: right;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="400px" height="126px">
		<g:south size="26.0">
			<g:FlowPanel width="100%" height="100%">
				<g:Button addStyleNames="{style.floatRight}" ui:field="btnCancel"
					width="90px" />
				<g:Button addStyleNames="{style.floatRight}" ui:field="btnDelete"
					width="90px" />
			</g:FlowPanel>
		</g:south>
		<g:north size="50.0">
			<g:SimplePanel>
				<g:Label text="New Label" ui:field="askLabel" height="100%"
					addStyleNames="{style.floatLeft}" width="100%" />
			</g:SimplePanel>
		</g:north>
		<g:center>
			<g:FlowPanel>
				<g:CheckBox text="New CheckBox" ui:field="deleteServicesField"
					height="18px" addStyleNames="{style.floatLeft}" />
			</g:FlowPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 