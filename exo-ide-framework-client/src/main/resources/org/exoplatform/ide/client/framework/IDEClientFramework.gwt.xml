<module>
   <!-- Inherit the core Web Toolkit stuff.                        -->
   <inherits name='com.google.gwt.user.User' />

   <!-- Inherit the default GWT style sheet.  You can change       -->
   <!-- the theme of your GWT application by uncommenting          -->
   <!-- any one of the following lines.                            -->
   <inherits name='com.google.gwt.user.Debug' />
   <inherits name='com.google.gwt.xml.XML' />
   <inherits name='com.google.gwt.json.JSON' />
   <inherits name="com.google.gwt.resources.Resources" />
   <inherits name='org.exoplatform.gwtframework.commons.Commons' />
   
   <generate-with class="org.exoplatform.ide.generator.ControlAnnotationMapGenerator">
      <when-type-assignable class="org.exoplatform.ide.client.framework.annotation.ClassAnnotationMap" />
   </generate-with>

   <source path="annotation" />
   <source path="application" />
   <source path="codeassistant" />
   <source path="configuration" />
   <source path="control" />
   <source path="editor" />
   <source path="event" />
   <source path="form" />
   <source path="module" />
   <source path="navigation" />
   <source path="output" />
   <source path="settings" />
   <source path="ui" />
   <source path="userinfo" />
   <source path="vfs" />
</module>
