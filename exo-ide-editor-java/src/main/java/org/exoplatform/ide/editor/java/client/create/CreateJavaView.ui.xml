<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component">
	<ui:with field='tex' type='org.exoplatform.ide.editor.java.client.JavaConstants'/>
	<ui:style>
	</ui:style>
   <f:Border borderColor="transparent" margin="5">
   	<f:layer left="0px" top="0px" bottom="0px" right="0px">
   		<g:LayoutPanel width="" height="">
   			<g:layer left="10px" width="58px" top="18px" height="14px">
   				<g:Label text="{tex.createJCLabelName}"/>
   			</g:layer>
   			<g:layer left="74px" top="11px" height="27px" right="18px">
   				<f:TextInput ui:field="nameInput" width="100%" height="100%"/>
   			</g:layer>
   			<g:layer left="10px" width="58px" top="50px" height="14px">
   				<g:Label text="{tex.createJavaType}"/>
   			</g:layer>
   			<g:layer width="90px" top="89px" height="22px" right="128px">
   				<f:ImageButton ui:field="cancelButton"  text="{tex.createJCButtonCancel}"/>
   			</g:layer>
   			<g:layer width="90px" top="89px" height="22px" right="20px">
   				<f:ImageButton ui:field="createButton"  text="{tex.createJCButtonCreate}"/>
   			</g:layer>
   			<g:layer left="74px" top="44px" height="27px" right="18px">
   				<f:SelectItem ui:field="typeSelect" width="100%" height="100%"/>
   			</g:layer>
   		</g:LayoutPanel>
   	</f:layer>
   </f:Border>
</ui:UiBinder> 