<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   <modelVersion>4.0.0</modelVersion>
   <parent>
      <artifactId>exo-ide-parent</artifactId>
      <groupId>org.exoplatform.ide</groupId>
      <version>1.2.0-SNAPSHOT</version>
   </parent>
   <groupId>org.exoplatform.ide</groupId>
   <artifactId>exo-ide-extension-cloudbees</artifactId>
   <name>eXo IDE :: Extension :: CLOUDBEES</name>
   <dependencies>
      <dependency>
         <groupId>org.exoplatform.ide</groupId>
         <artifactId>exo-ide-maven</artifactId>
      </dependency>
      <dependency>
         <groupId>org.exoplatform.ide</groupId>
         <artifactId>exo-ide-extension-git</artifactId>
      </dependency>
      <dependency>
         <groupId>org.exoplatform.ide</groupId>
         <artifactId>exo-ide-extension-java</artifactId>
      </dependency>
      <dependency>
         <groupId>org.exoplatform.jcr</groupId>
         <artifactId>exo.jcr.component.core</artifactId>
         <exclusions>
            <exclusion>
               <groupId>xstream</groupId>
               <artifactId>xstream</artifactId>
            </exclusion>
         </exclusions>
      </dependency>
      <dependency>
         <groupId>com.cloudbees</groupId>
         <artifactId>cloudbees-api-client</artifactId>
      </dependency>
      <dependency>
         <groupId>javax.ws.rs</groupId>
         <artifactId>jsr311-api</artifactId>
      </dependency>
      <dependency>
         <groupId>org.exoplatform.ide</groupId>
         <artifactId>exo-ide-framework-client</artifactId>
      </dependency>
      <dependency>
         <groupId>junit</groupId>
         <artifactId>junit</artifactId>
         <scope>test</scope>
      </dependency>
   </dependencies>
   <build>
      <resources>
         <resource>
            <directory>src/main/java</directory>
         </resource>
         <resource>
            <directory>src/main/resources</directory>
         </resource>
      </resources>
      <sourceDirectory>src/main/java</sourceDirectory>
      <outputDirectory>target/classes</outputDirectory>
   </build>
</project>
