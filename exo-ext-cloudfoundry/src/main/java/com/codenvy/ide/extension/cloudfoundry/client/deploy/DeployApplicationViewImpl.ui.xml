<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="100%" height="100%"
		ui:field="cloudFoundryPanel">
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="150.0">
					<g:Label text="Target:" height="18px" />
				</g:west>
				<g:center>
					<g:ListBox width="100%" height="22px" ui:field="targetField" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="150.0">
					<g:Label text="Application name:" height="18px" />
				</g:west>
				<g:center>
					<g:TextBox width="100%" height="22px" ui:field="nameField" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="150.0">
					<g:Label text="Location:" height="18px" />
				</g:west>
				<g:center>
					<g:TextBox width="100%" height="22px" ui:field="urlField" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
	</g:DockLayoutPanel>
</ui:UiBinder> 