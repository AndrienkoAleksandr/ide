<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="locale"
		type="com.codenvy.ide.extension.cloudfoundry.client.CloudFoundryLocalizationConstant"></ui:with>

	<ui:style>
		.alignRight {
			float: right;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="400px" height="130px">
		<g:south size="26.0">
			<g:FlowPanel>
				<g:Button height="22px" width="90px" styleName="{style.alignRight}" text="{locale.cancelButton}" ui:field="btnCancel" />
				<g:Button height="22px" width="90px" styleName="{style.alignRight}" text="{locale.okButton}" ui:field="btnOk" />
			</g:FlowPanel>
		</g:south>
		<g:center>
			<g:FlowPanel>
				<g:Label text="New Label" width="100%" ui:field="title"/>
				<g:TextBox width="98%" height="22px" ui:field="property"/>
			</g:FlowPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>