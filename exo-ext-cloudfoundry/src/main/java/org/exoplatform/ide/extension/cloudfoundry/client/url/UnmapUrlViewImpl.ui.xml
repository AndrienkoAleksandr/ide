<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">

	<ui:with field="locale"
		type="org.exoplatform.ide.extension.cloudfoundry.client.CloudFoundryLocalizationConstant"></ui:with>

	<ui:style>
		.alignRight {
			float: right;
		}
		
		.spacingLeft {
			margin-left: 10px;
		}
		
		.spacing {
			margin-top: 10px;
			margin-bottom: 10px;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="430px" height="270px">
		<g:south size="26.0">
			<g:FlowPanel width="100%" height="100%">
				<g:Button width="100px" height="26px" text="" addStyleNames="{style.alignRight}"
					ui:field="btnClose" />
			</g:FlowPanel>
		</g:south>
		<g:south size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:east size="100.0">
					<g:Button width="90px" height="26px" text=""
						addStyleNames="{style.spacingLeft}" ui:field="btnMap" />
				</g:east>
				<g:center>
					<g:TextBox height="22px" width="98%" ui:field="mapUrl" />
				</g:center>
			</g:DockLayoutPanel>
		</g:south>
		<g:center>
			<g:ScrollPanel width="100%" height="100%">
				<p1:CellTable width="100%" height="100%" ui:field="urlsTable" />
			</g:ScrollPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 