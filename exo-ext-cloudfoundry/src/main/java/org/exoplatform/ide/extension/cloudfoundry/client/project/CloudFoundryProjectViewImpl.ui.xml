<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with field="locale"
		type="org.exoplatform.ide.extension.cloudfoundry.client.CloudFoundryLocalizationConstant"></ui:with>
	<ui:with field='resource'
		type='org.exoplatform.ide.extension.cloudfoundry.client.CloudFoundryResources' />

	<ui:style>
		.close {
			margin-right: auto;
		}
		
		.left {
			float: left;
		}
		
		.space {
			margin-right: 5px;
		}
		
		.right {
			float: right;
		}
		
		.labelH {
			margin-right: 5px;
			font-size: 11px;
			font-family: Verdana, Bitstream Vera Sans, sans-serif;
			float: left;
			font-weight: bold;
		}
		
		.link {
			font-size: 11px;
			font-family: Verdana, Bitstream Vera Sans, sans-serif;
			overflow-x: hidden;
			white-space: nowrap;
		}
		
		.padding {
			padding: 15px;
		}
		
		.actions {
			border-top: 1px solid #DBDFE5;
			padding-top: 5px;
		}
		
		.textinput {
			background-color: #FFFFFF !important;
			color: #000000 !important;
		}
		
		.middle {
			margin-top: 3px;
		}
	</ui:style>
	<g:DockLayoutPanel unit="PX" width="500px" height="330px">
		<g:south size="26.0">
			<g:FlowPanel>
				<g:Button text="{locale.closeButton}" styleName="{style.right}"
					ui:field="btnClose" />
			</g:FlowPanel>
		</g:south>
		<!--<g:north size="30.0"> <g:FlowPanel width="100%" height="100%" > <g:Label 
			text="{locale.manageProjectApplication}" addStyleNames="{style.labelH} {style.middle}" 
			width="80px" /> <g:TextBox width="370px" height="22px" addStyleNames="{style.textinput} 
			{style.left} {style.space}" /> <g:Button width="22px" height="22px" text="" 
			styleName="{style.left}" title="{locale.manageProjectProperties}"><img src="{resource.propertiesButton.getURL}"></img></g:Button> 
			</g:FlowPanel> </g:north> -->
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectApplication}"
						addStyleNames="{style.labelH} {style.middle}" />
				</g:west>
				<g:east size="30.0">
					<g:Button width="22px" height="22px" text=""
						addStyleNames="{style.space}" title="{locale.manageProjectProperties}"
						ui:field="btnInfo">
						<img src="{resource.propertiesButton.getURL}"></img>
					</g:Button>
				</g:east>
				<g:center>
					<g:TextBox height="22px" addStyleNames="{style.textinput} {style.space}"
						ui:field="applicationName" width="97%" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<!--<g:north size="30.0"> <g:FlowPanel width="100%" height="100%"> <g:Label 
			text="{locale.manageProjectUrls}" addStyleNames="{style.labelH} {style.middle}" 
			width="80px" /> <g:Anchor href="" text="" addStyleNames="{style.left} {style.link} 
			{style.space}" target="_blank" width="370px" height="22px" /> <g:Button width="22px" 
			height="22px" text="..." styleName="{style.left}" title="more" /> </g:FlowPanel> 
			</g:north> -->
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectUrls}" addStyleNames="{style.labelH} {style.middle}" />
				</g:west>
				<g:east size="30.0">
					<g:Button width="22px" height="22px" text="..." styleName="{style.left}"
						title="more" ui:field="btnEditUrl" />
				</g:east>
				<g:center>
					<g:Anchor href="" text=""
						addStyleNames="{style.left} {style.link} {style.space}" target="_blank"
						height="22px" ui:field="url" width="97%" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<!--<g:north size="30.0"> <g:FlowPanel width="100%" height="100%"> <g:Label 
			text="{locale.manageProjectMemory}" addStyleNames="{style.labelH} {style.middle}" 
			width="80px" /> <g:TextBox addStyleNames="{style.textinput} {style.left} 
			{style.space}" height="22px" width="370px" /> <g:Button width="22px" height="22px" 
			styleName="{style.left}" title="{locale.manageProjectEditMemory}" /> </g:FlowPanel> 
			</g:north> -->
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectMemory}" addStyleNames="{style.labelH} {style.middle}" />
				</g:west>
				<g:east size="30.0">
					<g:Button width="22px" height="22px" styleName="{style.left}"
						title="{locale.manageProjectEditMemory}" ui:field="btnEditMemory" />
				</g:east>
				<g:center>
					<g:TextBox addStyleNames="{style.textinput} {style.left} {style.space}"
						height="22px" ui:field="memory" width="97%" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectInstances}"
						addStyleNames="{style.labelH} {style.middle}" />
				</g:west>
				<g:east size="30.0">
					<g:Button width="22px" height="22px" styleName="{style.left}"
						title="{locale.manageProjectEditInstances}" ui:field="btnEditInstances" />
				</g:east>
				<g:center>
					<g:TextBox addStyleNames="{style.textinput} {style.left} {style.space}"
						height="22px" ui:field="instances" width="97%" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectStack}" addStyleNames="{style.labelH}" />
				</g:west>
				<g:center>
					<g:Label text="" addStyleNames="{style.left}" ui:field="stack" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectModel}" addStyleNames="{style.labelH}" />
				</g:west>
				<g:center>
					<g:Label text="" addStyleNames="{style.left}" ui:field="model" />
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectStatus}" addStyleNames="{style.labelH}" />
				</g:west>
				<g:center>
					<g:FlowPanel>
						<g:Label addStyleNames="{style.left} {style.space} {style.middle}"
							width="70px" ui:field="status" />
						<g:Button width="22px" height="22px" text=""
							title="{locale.manageProjectStart}" styleName="{style.left} {style.space}"
							ui:field="btnStart" />
						<g:Button width="22px" height="22px" text=""
							title="{locale.manageProjectStop}" styleName="{style.left} {style.space}"
							ui:field="btnStop" />
						<g:Button width="22px" height="22px" text=""
							title="{locale.manageProjectRestart}" styleName="{style.left}"
							ui:field="btnRestart" />
					</g:FlowPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<g:north size="30.0">
			<g:DockLayoutPanel unit="PX" width="100%" height="100%">
				<g:west size="80.0">
					<g:Label text="{locale.manageProjectActions}"
						addStyleNames="{style.labelH}" />
				</g:west>
				<g:center>
					<g:FlowPanel>
						<g:Button height="22px" width="70px" text="{locale.updateButton}"
							styleName="{style.left} {style.space}" ui:field="btnUpdate" />
						<g:Button height="22px" width="70px" text="{locale.deleteButton}"
							styleName="{style.left} {style.space}" ui:field="btnDelete" />
						<g:Button height="22px" width="70px" text="{locale.servicesButton}"
							styleName="{style.left} {style.space}" ui:field="btnServices" />
						<g:Button height="22px" width="70px" text="{locale.logsButton}"
							styleName="{style.left}" ui:field="btnLogs" />
					</g:FlowPanel>
				</g:center>
			</g:DockLayoutPanel>
		</g:north>
		<!--<g:north size="30.0"> <g:FlowPanel width="100%" height="100%" /> </g:north> 
			<g:north size="30.0"> <g:FlowPanel width="100%" height="100%" /> </g:north> 
			<g:north size="30.0"> <g:FlowPanel width="100%" height="100%" /> </g:north> 
			<g:north size="30.0"> <g:FlowPanel width="100%" height="100%" /> </g:north> 
			<g:north size="30.0"> <g:FlowPanel width="100%" height="100%" /> </g:north> -->
	</g:DockLayoutPanel>
</ui:UiBinder> 