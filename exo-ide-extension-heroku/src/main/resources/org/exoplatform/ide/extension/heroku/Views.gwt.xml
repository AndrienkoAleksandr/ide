<module>
   <inherits name='com.google.gwt.user.User' />
   <replace-with class="org.exoplatform.ide.extension.heroku.client.login.LoginView">
      <when-type-is class="org.exoplatform.ide.extension.heroku.client.login.LoginPresenter.Display" />
   </replace-with>
   
   <replace-with class="org.exoplatform.ide.extension.heroku.client.create.CreateApplicationView">
      <when-type-is class="org.exoplatform.ide.extension.heroku.client.create.CreateApplicationPresenter.Display" />
   </replace-with>
   
   <replace-with class="org.exoplatform.ide.extension.heroku.client.info.ApplicationInfoView">
      <when-type-is class="org.exoplatform.ide.extension.heroku.client.info.ApplicationInfoPresenter.Display" />
   </replace-with>
   
   <replace-with class="org.exoplatform.ide.extension.heroku.client.rename.RenameApplicationView">
      <when-type-is class="org.exoplatform.ide.extension.heroku.client.rename.RenameApplicationPresenter.Display" />
   </replace-with>
</module>
