<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component">
   <ui:with field='resource' type='org.exoplatform.ide.extension.heroku.client.HerokuClientBundle' />
   <ui:with field="locale" type="org.exoplatform.ide.extension.heroku.client.HerokuLocalizationConstant"></ui:with>
   <f:Border margin="7" borderColor="transparent">
      <g:VerticalPanel spacing="3" horizontalAlignment="center">
         <f:TextField ui:field="nameField" title="{locale.renameApplicationViewNameField}" width="100%" height="22px"></f:TextField>
         <g:HorizontalPanel height="40" spacing="5" horizontalAlignment="center" verticalAlignment="bottom">
            <f:IButton ui:field="renameButton" width="90" height="22" title="{locale.renameButton}" icon="{resource.okButton.getURL}" disableIcon="{resource.okButtonDisabled.getURL}"></f:IButton>
            <f:IButton ui:field="cancelButton" width="90" height="22" title="{locale.cancelButton}" icon="{resource.cancelButton.getURL}" disableIcon="{resource.cancelButtonDisabled.getURL}"></f:IButton>
         </g:HorizontalPanel>
      </g:VerticalPanel>
   </f:Border>
</ui:UiBinder> 