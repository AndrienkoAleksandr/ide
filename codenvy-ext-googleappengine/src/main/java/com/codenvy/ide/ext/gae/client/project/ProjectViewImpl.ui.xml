<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.codenvy.ide.ui">
    <ui:with field='resource' type='com.codenvy.ide.ext.gae.client.GAEResources'/>
    <ui:with field='constant' type='com.codenvy.ide.ext.gae.client.GAELocalization'/>
    <ui:style>
        .tabPanel {
            padding: 10px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="625px" height="350px">
        <g:south size="26.0">
            <g:FlowPanel>
                <c:Button ui:field="btnClose" width="50px" height="18px" text="{constant.closeButton}"
                           type="SUCCESS"/>
            </g:FlowPanel>
        </g:south>
        <g:north size="320.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:center>
                    <g:TabPanel width="270px" ui:field="applicationTabPanel" styleName="{style.tabPanel}"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>