<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <ui:with field='res' type='com.codenvy.ide.ext.gae.client.GAEResources'/>
    <ui:with field='constant' type='com.codenvy.ide.ext.gae.client.GAELocalization'/>
    <ui:style>
        .spacing {
            margin-bottom: 6px;
            margin-top: 10px;
        }

        .checkBoxPadding {
            padding-top: 2px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="100%" height="100%">
        <g:north size="50.0">
            <g:DockLayoutPanel unit="PX" width="100%" height="100%" addStyleNames="{style.spacing}">
                <g:west size="235.0">
                    <g:CheckBox ui:field="requiredAppId" text="Use existing Google App Engine Id:"
                                styleName="{style.checkBoxPadding}"/>
                </g:west>
                <g:center>
                    <g:TextBox ui:field="existedAppId" width="310px" height="18px"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:north>
    </g:DockLayoutPanel>
</ui:UiBinder>