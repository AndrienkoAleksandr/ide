    <build>

        <resources>
            <resource>
                <directory>src/main/resources</directory>
            </resource>
        </resources>

        <plugins>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-war-plugin</artifactId>
                <executions>
                    <execution>
                        <phase>verify</phase>
                        <goals>
                            <goal>war</goal>
                        </goals>
                        <configuration>
                            <packagingExcludes>**/classes/org/**,**/classes/gadget/**,
                                **/classes/default/**,**/classes/ide/**,
                                **/Commons.css,
                                **/hosted.html,**/applicationInitializer.js,**/init_preload_gadget.js,
                                **/IDEShell.html,**/IDEGadget.html,**/ShowCase.html,**/preload.html,
                                **/preloadIDE.js,
                                **/lib/exo-ide-jdt-core*,
                                **/lib/exo-jcr-services*,
                                **/lib/slf4j-log4j*,
                                **/lib/log4j*,
                                **/lib/commons-logging-api*,
                                **/lib/commons-logging*,
                                **/lib/servlet-api*,
                                **/lib/hsqldb.*,
                                **/lib/carol*,
                                **/lib/jacorb*,
                                **/lib/ow2*,
                                **/lib/irmi*,
                                **/lib/avalon*,
                                **/lib/logkit*,
                                **/lib/howl*,
                                **/lib/hibernate*,
