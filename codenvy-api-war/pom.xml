                                **/lib/gwt-user*,
                                **/lib/gwt-dev*,
                                **/lib/postgresql*,
                                **/lib/tools.jar,
                                **/lib/guice-2.0.jar,
                                **/lib/antlr-2.*,
                                **/lib/xstream-1.0.2.jar,
                                **/lib/xml-apis*
                            </packagingExcludes>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
