<module>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
	<inherits name="com.google.gwt.http.HTTP" />
	<inherits name='com.google.gwt.user.Debug' />
	<inherits name='com.google.gwt.xml.XML' />
	<inherits name="com.google.gwt.i18n.I18N" />
	<!-- extend-property name="locale" values="ua" />
	<extend-property name="locale" values="ru" />
	<extend-property name="locale" values="en" /-->

	<inherits name='com.smartgwt.tools.SmartGwtTools'/>

	<!--  Inherit the SmartGWT Toolkit library configuration. -->
	<script src="applicationInitializer.js" />

	<inherits name="com.smartgwt.SmartGwt" />
	<!--
		<script src="init_smartgwt_for_gadget.js" />
		<inherits	name="com.smartgwt.SmartGwtNoScript"/>
	-->

	<!-- load GWT Commons Libraries -->
	<inherits name='org.exoplatform.gwt.commons.Commons' />

	<!-- Specify the app entry point class. -->
	<entry-point class='org.exoplatform.ideall.client.IDEApplication' />
	<source path="client" excludes="DevToolGadget.java" />

	<!-- Specify the application specific style sheet. -->
	<stylesheet src='IDE.css' />
	<stylesheet src='eXoStyle/eXoStyle.css' />

	<!-- Load CodeMirror editor files.
  <stylesheet src='codemirror-0.65/css/editor.css' />
  <script src="codemirror-0.65/js/codemirror.js" />   
	-->

</module>
