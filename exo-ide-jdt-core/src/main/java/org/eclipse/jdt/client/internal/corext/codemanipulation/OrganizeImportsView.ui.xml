<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<f:Border borderColor="transparent" margin="5">
		<f:layer top="0px" height="50px" left="0px" right="0px">
			<g:DockLayoutPanel unit="PX">
				<g:south size="25.0">
					<f:TextInput ui:field="filterTextInput" name="ideOrganizeImportFiterInput" width="100%" />
				</g:south>
				<g:west size="160.0">
					<g:Label text="Choose type to import:" height="24"/>
				</g:west>
				<g:east size="87.0">
					<g:Label text="Page 1 of 1" ui:field="pageLabel"/>
				</g:east>
			</g:DockLayoutPanel>
		</f:layer>
		<f:layer left="0px" height="32px" right="3px" bottom="2px">
			<g:LayoutPanel width="" height="">
				<g:layer left="15px" width="90px" top="0px" height="22px">
					<f:ImageButton ui:field="backButton" text=" &lt; Back" buttonId="ideOrganizeImportBack"/>
				</g:layer>
				<g:layer left="125px" width="90px" top="0px" height="22px">
					<f:ImageButton ui:field="nextButton" text="Next &gt;" buttonId="ideOrganizeImportNext"/>
				</g:layer>
				<g:layer left="232px" width="90px" top="0px" height="22px">
					<f:ImageButton ui:field="cancelButton" text="Cancel" buttonId="ideOrganizeImportCancel"/>
				</g:layer>
				<g:layer left="339px" width="90px" top="0px" height="22px">
					<f:ImageButton ui:field="finishButton" text="Finish" buttonId="ideOrganizeImportFinish"/>
				</g:layer>
			</g:LayoutPanel>
		</f:layer>
		<f:layer left="0px" right="0px" top="55px" bottom="40px">
			<f:Border borderSize="1">
						<f:layer left="0px" top="0px" right="0px" bottom="1px">
							<p1:CellList ui:field="cellList" focus="true"/>
						</f:layer>
					</f:Border>
		</f:layer>
	</f:Border>
</ui:UiBinder> 