<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='RubyEditor'>
   <!-- Inherit the core Web Toolkit stuff. -->
   <inherits name='com.google.gwt.user.User' />
   <inherits name="com.google.gwt.http.HTTP" />
   <inherits name='com.google.gwt.user.Debug' />
   <inherits name='com.google.gwt.json.JSON' />

   <!-- Other module inherits -->
   <inherits name='org.exoplatform.gwtframework.commons.Commons' />
   <inherits name='org.exoplatform.ide.client.framework.IDEClientFramework' />
   <inherits name='org.exoplatform.ide.editor.Editor'/>

   <!-- Specify the app entry point class. -->
   <entry-point class='org.exoplatform.ide.editor.ruby.client.RubyEditor' />

   <!-- Specify the paths for translatable code -->
   <source path='client' />
   <source path='shared' />

</module>
