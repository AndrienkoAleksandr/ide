<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component">
   <ui:with field='resource' type='org.exoplatform.ide.client.IDEImageBundle' />
   <ui:with field="locale" type="org.exoplatform.ide.client.IdeLocalizationConstant"></ui:with>
   <f:Border margin="7" borderColor="transparent">
      <g:VerticalPanel spacing="3" horizontalAlignment="center">
         <f:TextField ui:field="projectName" title="{locale.projectName}" width="100%" height="22px"></f:TextField>
         <f:TextField ui:field="projectType" title="{locale.projectType}" width="100%" height="22px"></f:TextField>
         <g:HorizontalPanel height="40" spacing="5" horizontalAlignment="center" verticalAlignment="bottom">
            <f:ImageButton ui:field="createButton" width="90" height="22" text="{locale.createButton}" imageResource="{resource.ok}" disabledImageResource="{resource.okDisabled}"></f:ImageButton>
            <f:ImageButton ui:field="cancelButton" width="90" height="22" text="{locale.cancelButton}" imageResource="{resource.cancel}" disabledImageResource="{resource.cancelDisabled}"></f:ImageButton>
         </g:HorizontalPanel>
      </g:VerticalPanel>
   </f:Border>
</ui:UiBinder> 