<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
             xmlns:g="urn:import:com.google.gwt.user.client.ui" 
             xmlns:f="urn:import:org.exoplatform.gwtframework.ui.client.component"
             xmlns:e="urn:import:org.exoplatform.ide.client.versioning">

   <ui:with field='resource' type='org.exoplatform.ide.client.IDEImageBundle' />
   <ui:with field="locale" type="org.exoplatform.ide.client.IdeLocalizationConstant"></ui:with>

   <f:VPanel>
      <f:Border margin="10">
         <e:VersionsGrid width="100%" ui:field="versionsGrid"></e:VersionsGrid>
      </f:Border>

      <g:HorizontalPanel height="30px" horizontalAlignment="center">
         <f:ImageButton ui:field="openVersionButton" width="90" height="22" text="{locale.openButton}" imageResource="{resource.ok}"
            disabledImageResource="{resource.okDisabled}"></f:ImageButton>

         <f:ImageButton ui:field="closeButton" width="90" height="22" text="{locale.closeButton}" imageResource="{resource.cancel}"
            disabledImageResource="{resource.cancelDisabled}"></f:ImageButton>
      </g:HorizontalPanel>
   </f:VPanel>
</ui:UiBinder> 