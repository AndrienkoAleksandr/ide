<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
   xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
   <ui:style>
      /* Add CSS here. See the GWT docs on UI Binder for more details */
      .important {
      	font-weight: bold;
      }
      
      .position {
      	position: static;
      }
   </ui:style>
      <g:DockLayoutPanel unit="PX" width="100%" height="100%">
         <g:north size="22">
            <g:SimplePanel width="100%" ui:field="topPanel" />
         </g:north>
         <g:north size="22">
         	<g:SimplePanel width="100%" ui:field="secondTopPanel"/>
         </g:north>
         <g:center>
            <g:SplitLayoutPanel ui:field="splitPanel">
               <g:west size="240">
                  <g:SimplePanel ui:field="leftPanel" width="100%" height="100%">
                     <!-- p1:CellTree width="100%" height="100%" ui:field="cellTree" / -->
                  </g:SimplePanel>
               </g:west>
               <g:center>
                  <g:SimplePanel ui:field="centerPanel" styleName="{style.position}" />
               </g:center>
               <g:east size="240">
                  <g:SimplePanel ui:field="rightPanel" width="100%" height="100%"></g:SimplePanel>
               </g:east>
            </g:SplitLayoutPanel>
         </g:center>
      </g:DockLayoutPanel>
</ui:UiBinder> 