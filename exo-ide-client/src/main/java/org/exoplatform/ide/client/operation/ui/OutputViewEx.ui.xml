<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:org.exoplatform.gwtframework.ui.client.component"
	xmlns:t="urn:import:org.exoplatform.gwtframework.ui.client.toolbar">

	<c:VPanel>
		<t:Toolbar height="28px" ui:field="toolbar"></t:Toolbar>

		<g:ScrollPanel ui:field="scrollPanel">
			<g:FlowPanel width="100%" ui:field="contentPanel"></g:FlowPanel>
		</g:ScrollPanel>
	</c:VPanel>

</ui:UiBinder>
 