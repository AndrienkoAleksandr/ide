	<ui:style>
		
		.managerPanel
		{
			position: absolute;
			left:100px;
			top:50px;
			width: 200px;
			height: 300px;
			background: #FFEEAA;
			opacity: 0.5;
		}
		
	</ui:style>

	<g:HTMLPanel styleName="{style.managerPanel}">
	</g:HTMLPanel>

</ui:UiBinder>
 