<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:org.exoplatform.gwtframework.ui.client.component"
	xmlns:w="urn:import:org.exoplatform.ide.client.workspace.ui"
	>
	<ui:with type="org.exoplatform.ide.client.IdeLocalizationConstant" field="idelocal"></ui:with>
	
	<c:VPanel>
	
		<c:Border margin="5">
			<w:VFSListGrid ui:field="entryPointListGrid"></w:VFSListGrid>
		</c:Border>

		<c:Panel height="40px">
		
			<g:HorizontalPanel spacing="5" width="100%" height="100%">
			
				<g:cell horizontalAlignment='ALIGN_RIGHT' verticalAlignment="ALIGN_MIDDLE">
					<c:ImageButton text="{idelocal.okButton}" imageName="ok" ui:field="okButton" buttonId="ideEntryPointOkButton"></c:ImageButton>
				</g:cell>
				
				<g:cell horizontalAlignment='ALIGN_LEFT' verticalAlignment="ALIGN_MIDDLE">
					<c:ImageButton text="{idelocal.cancelButton}" imageName="cancel" ui:field="cancelButton" buttonId="ideEntryPointCancelButton"></c:ImageButton>
				</g:cell>
				
			</g:HorizontalPanel>
			
		</c:Panel>
		
	</c:VPanel>
	
</ui:UiBinder>
