	<replace-with class="org.exoplatform.ide.client.properties.ui.PropertiesView">
 -->
	<replace-with class="org.exoplatform.ide.client.properties.ui.PropertiesViewEx">
		<when-type-is class="org.exoplatform.ide.client.properties.PropertiesPresenter.Display" />
	</replace-with>

	<replace-with class="org.exoplatform.ide.client.navigation.ui.DeleteItemView">
		<when-type-is class="org.exoplatform.ide.client.navigation.DeleteItemsPresenter.Display" />
	</replace-with>

	<replace-with class="org.exoplatform.ide.client.navigation.ui.GetItemURLView">
		<when-type-is class="org.exoplatform.ide.client.navigation.GetItemURLPresenter.Display" />
	</replace-with>

