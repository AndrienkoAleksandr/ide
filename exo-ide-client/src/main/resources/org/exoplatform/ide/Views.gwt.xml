<module>
	<inherits name='com.google.gwt.user.User' />

	<replace-with class="org.exoplatform.ide.client.navigation.ui.WorkspaceViewExtended">
		<when-type-is class="org.exoplatform.ide.client.navigation.WorkspacePresenter.Display" />
	</replace-with>   
	
	<replace-with class="org.exoplatform.ide.client.navigation.ui.SearchFilesView">
		<when-type-is class="org.exoplatform.ide.client.navigation.SearchFilesPresenter.Display" />
	</replace-with>

	<replace-with class="org.exoplatform.ide.client.navigation.ui.SearchResultsViewExtended">
		<when-type-is class="org.exoplatform.ide.client.navigation.SearchResultsPresenter.Display" />
	</replace-with>

