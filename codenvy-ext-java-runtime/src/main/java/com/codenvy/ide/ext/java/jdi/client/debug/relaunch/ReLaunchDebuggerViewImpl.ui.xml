<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:c='urn:import:com.codenvy.ide.ui'>
    <ui:with field='locale' type='com.codenvy.ide.ext.java.jdi.client.JavaRuntimeLocalizationConstant'/>
    <ui:with field='res' type='com.codenvy.ide.ext.java.jdi.client.JavaRuntimeResources'/>
    <ui:style>
        .alignBtn {
            float: right;
        }

        .emptyBorder {
            margin: 6px;
        }
    </ui:style>
    <g:DockLayoutPanel unit="PX" width="320px" height="130px" addStyleNames="{style.emptyBorder}">
        <g:south size="26">
            <g:FlowPanel>
                <c:Button ui:field="btnCancel" text="{locale.cancelButton}" type="DANGER" size="SMALL" width="50px"
                          addStyleNames="{style.alignBtn}"/>
            </g:FlowPanel>
        </g:south>
        <g:center>
            <g:FlowPanel>
                <g:Label text="{locale.debug}"/>
            </g:FlowPanel>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>